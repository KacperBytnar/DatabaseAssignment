@page
@model DatabaseAssingment.Pages.StudentPages.StudentWaitingListModel
@{
}
<table class="table">
    <thead>
        <tr>
            <th>  Student Number  </th>
            <th>  Name  </th>
            <th>  Addres </th>
            <th>  Has a room?</th>
            <th>  Registration Date </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.waitingList)
        {
            <tr>
                <td>
                    @item.Student_No
                </td>
                <td>
                    @item.SName
                </td>
                <td>
                    @item.SAddress
                </td>
                <td>
                    @item.Has_Room
                </td>
                <td>
                    @item.RegistrationDate
                </td>
            </tr>
        }
    </tbody>
</table>